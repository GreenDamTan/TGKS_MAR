<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" 
	"http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package namespace="/mar" name="mar" extends="struts-default">
		
		<!-- invite -->
		
		<action name="invitePage" class="com.moemao.tgks.mar.invite.InveteAction" method="invitePage">
            <result name="success">/WEB-INF/jsp/mar/invite/invite.jsp</result>
        </action>
        
		<action name="invite" class="com.moemao.tgks.mar.invite.InveteAction" method="invite">
			<result name="success">/WEB-INF/jsp/common/success.jsp</result>
            <result name="error">/WEB-INF/jsp/common/error.jsp</result>
		</action>
		
	</package>
</struts>